{% extends 'base.html.twig' %}

{% block title %}Hello Association
{% endblock %}

{% block body %}
	{% for asso in association %}
		<h1>{{ asso.name }}</h1>
		<hr>
		<div class="row">
			<div class="col-6">
				<p>{{ asso.email }}</p>
			</div>
			<div class="col-6">
				<p>{{ asso.telephone }}</p>
			</div>
		</div>
	{% endfor %}
{% endblock %}
